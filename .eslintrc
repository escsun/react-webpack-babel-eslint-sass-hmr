{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "rules": {
    "strict": 1,
    "camelcase": 1,
    "quotes": ["error", "double"],
    "no-extra-semi": ["error"],
    "semi": ["error", "always"]
  }
}