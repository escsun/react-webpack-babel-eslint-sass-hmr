{
  "name": "react-webpack-babel-eslint-sass-hmr",
  "version": "1.0.0",
  "description": "Starter kit for React, Webpack, Babel, EsLint, Sass and Hot Module Replacment (HMR)",
  "main": "index.jsx",
  "author": "escsun <escsunarcher@gmail.com>",
  "license": "GPL-3.0",
  "scripts": {
    "start": "better-npm-run dev",
    "build": "better-npm-run build"
  },
  "betterScripts": {
    "dev": {
      "command": "webpack-dev-server",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "build": {
      "command": "webpack --display-chunks --progress",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.34",
    "@babel/plugin-transform-runtime": "^7.0.0-beta.34",
    "@babel/preset-env": "^7.0.0-beta.34",
    "@babel/preset-react": "^7.0.0-beta.34",
    "@babel/preset-stage-0": "^7.0.0-beta.34",
    "@babel/runtime": "^7.0.0-beta.34",
    "babel-eslint": "7",
    "babel-loader": "^8.0.0-beta.0",
    "better-npm-run": "^0.1.0",
    "clean-webpack-plugin": "^0.1.17",
    "copy-webpack-plugin": "^4.2.3",
    "css-loader": "^0.28.7",
    "eslint": "3.x",
    "eslint-plugin-react": "^7.5.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "html-webpack-plugin": "^2.30.1",
    "node-sass": "^4.5.3",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.3"
  },
  "dependencies": {
    "@babel/preset-es2017": "^7.0.0-beta.34",
    "axios": "^0.17.0",
    "core-js": "^2.5.1",
    "history": "^4.7.2",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-hot-loader": "^3.1.1",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.8",
    "redux": "^3.7.2",
    "redux-devtools-extension": "^2.13.2"
  }
}
